show databases;
CREATE DATABASE mlb;
USE mlb;
CREATE TABLE mlb (Name VARCHAR(255) NOT NULL, Team VARCHAR(3) NOT NULL, Position VARCHAR(25) NOT NULL, Height INT(3), Weight INT(3), Age DECIMAL(4, 2));
LOAD DATA LOCAL INFILE '/home/user/database/week_3/MLB.txt' INTO TABLE mlb FIELDS TERMINATED BY '\t' ENCLOSED BY '"' LINES TERMINATED BY '\n' IGNORE 1 ROWS;
SELECT * FROM mlb;
ALTER TABLE mlb ADD COLUMN id INT AUTO_INCREMENT PRIMARY KEY FIRST;
